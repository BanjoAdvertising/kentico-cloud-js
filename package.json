{
  "name": "kentico-cloud-delivery-typescript-sdk",
  "version": "4.0.0-beta.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Enngage/KenticoCloudDeliveryTypeScriptSDK"
  },
  "author": "richard sustek",
  "description": "Official Kentico Cloud Delivery SDK",
  "keywords": [
    "Kentico",
    "Kentico API",
    "Kentico Javascript",
    "Kentico SDK",
    "Kentico Cloud",
    "Kentico Cloud SDK",
    "Kentico Cloud Typescript",
    "Kentico Cloud Javascript",
    "Kentico Cloud Delivery",
    "Kentico Cloud Delivery SDK",
    "Kentico Delivery",
    "Kentico Delivery SDK"
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "test": "karma start karma.conf.js",
    "dev-test": "karma start karma-dev.conf.js",
    "travis-test": "karma start karma-travis.conf.js",
    "prepublishOnly": "npm run set-sdk-version && npm run check-version && npm run build:all && npm run travis-test && node test/test-completed",
    "set-sdk-version": "node ./misc/set-sdk-version.js",
    "check-version": "node ./misc/check-publish-version",
    "webpack:dev": "webpack --mode development --config webpack.config.js --progress --colors",
    "webpack:prod": "webpack --mode production --config webpack.config.js --progress --colors",
    "publish:beta": "npm publish --tag=next",
    "build-umd": "npm run webpack:dev && npm run webpack:prod",
    "build-es2015": "tsc -m es2015 --outDir _es2015",
    "build-commonjs": "tsc -m commonjs --outDir _commonjs",
    "build:all": "npm run build-umd && npm run build-es2015 && npm run build-commonjs && npm run build-umd"
  },
  "private": false,
  "dependencies": {
    "xhr": "^2.4.1",
    "reflect-metadata": "~0.1.12",
    "rxjs": "~6.0.0-rc.0"
  },
  "devDependencies": {
    "@types/jasmine": "2.8.6",
    "@types/node": "9.6.2",
    "core-js": "2.5.4",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-coverage-istanbul-reporter": "1.4.2",
    "karma-firefox-launcher": "1.1.0",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-typescript": "3.0.12",
    "karma-typescript-es6-transform": "1.0.4",
    "karma-webpack": "3.0.0",
    "lcov-parse": "1.0.0",
    "ts-loader": "4.1.0",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "typescript": "2.7.2",
    "url-parse": "1.2.0",
    "webpack": "4.4.1",
    "webpack-cli": "2.0.13"
  },
  "main": "./commonjs/index.js",
  "types": "./commonjs/index.d.ts"
}