{
  "name": "kentico-cloud-delivery-typescript-sdk",
  "version": "2.7.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/Enngage/KenticoCloudDeliveryTypeScriptSDK"
  },
  "author": "richard sustek",
  "description": "Official Kentico Cloud Delivery SDK",
  "keywords": [
    "Kentico",
    "Kentico API",
    "Kentico Javascript",
    "Kentico SDK",
    "Kentico Cloud",
    "Kentico Cloud SDK",
    "Kentico Cloud Typescript",
    "Kentico Cloud Javascript",
    "Kentico Cloud Delivery",
    "Kentico Cloud Delivery SDK",
    "Kentico Delivery",
    "Kentico Delivery SDK"
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "test": "karma start karma.conf.js",
    "dev-test": "karma start karma-dev.conf.js",
    "travis-test": "karma start karma-travis.conf.js",
    "prepublishOnly": "npm run build:prod && npm run travis-test && node test/test-completed",
    "new-patch": "npm version patch && npm run update-sdk-info",
    "new-minor": "npm version minor && npm run update-sdk-info",
    "new-major": "npm version major && npm run update-sdk-info",
    "update-sdk-info": "node ./misc/update-sdk-info-tasks.js",
    "webpack:dev": "webpack --mode development --config webpack.config.js --progress --colors",
    "webpack:prod": "webpack --mode production --config webpack.config.js --progress --colors",
    "build:dev": "tsc && tsc -m es6 --outDir dist-esm && npm run webpack:dev",
    "build:prod": "tsc && tsc -m es6 --outDir dist-esm && npm run webpack:prod",
    "publish:beta": "npm publish --tag=next"
  },
  "private": false,
  "dependencies": {
    "awesome-typescript-loader": "^4.0.1",
    "parse5": "4.0.0",
    "reflect-metadata": "0.1.12",
    "rx-dom": "7.0.3",
    "source-map-loader": "^0.2.3"
  },
  "peerDependencies": {
    "rxjs": "~5.5.7"
  },
  "devDependencies": {
    "@types/jasmine": "2.8.6",
    "@types/node": "9.6.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "core-js": "2.5.3",
    "gulp": "3.9.1",
    "gulp-clean": "0.4.0",
    "gulp-typescript": "4.0.1",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-coverage-istanbul-reporter": "1.4.2",
    "karma-firefox-launcher": "1.1.0",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-typescript": "3.0.12",
    "karma-typescript-es6-transform": "1.0.4",
    "karma-webpack": "3.0.0",
    "lcov-parse": "1.0.0",
    "merge2": "1.2.1",
    "ts-loader": "4.1.0",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "typescript": "2.7.2",
    "url-parse": "1.2.0",
    "webpack": "4.2.0",
    "webpack-cli": "2.0.13"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
